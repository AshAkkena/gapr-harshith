<p id="notice"><%= notice %></p>



<table class="apa">
<caption>Module Lookups</caption>
  <thead>
    <tr>
      <th>Period</th>
      <th>Abbreviated curriculum</th>
      <th>Delivery sequence</th>
      <th>Basic name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @module_lookups.each do |module_lookup| %>
      <tr>
        <td><%= module_lookup.period %></td>
        <td><%= module_lookup.abbreviated_curriculum %></td>
        <td><%= module_lookup.delivery_sequence %></td>
        <td><%= module_lookup.basic_name %></td>
        <td><%= link_to 'Show', module_lookup %></td>
        <td><%= link_to 'Edit', edit_module_lookup_path(module_lookup) %></td>
        <td><%= link_to 'Destroy', module_lookup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Module Lookup', new_module_lookup_path %>
